{
  "name": "lsr-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "checkAll": "yarn run lint --fix && yarn run flow && yarn run test --updateSnapshot --watchAll=0",
    "lint": "eslint . --ext .js",
    "flow": "flow",
    "test": "react-scripts test",
    "clean": "rm -rf build dist",
    "build": "yarn run clean && react-static build",
    "serve": "serve dist -p 3000",
    "deploy": "yarn run build && firebase deploy",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "lodash": "4.17.11",
    "material-ui": "1.0.0-beta.32",
    "nuka-carousel": "4.2.1",
    "ramda": "0.25.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-jss": "8.6.1",
    "react-static": "cooperka/react-static#2018-10-22-uglifyPlugin",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-cooperka": "0.2.11",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "firebase-tools": "5.1.1",
    "flow-bin": "0.83.0",
    "flow-typed": "2.5.1",
    "react-scripts": "2.0.5",
    "react-test-renderer": "16.5.2",
    "serve": "10.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
